myText[0] = "Sensei! je suis arrivée"
myText[1] = "Purakushisu, que fais-tu ici c'est dangereux. "
myText[2] = "Dairoku m'a demandé de venir ici pour aider le village."
myText[3] = "C'est bon, j'ai fais déplacer les habitants du village, ils sont en sécurité."
myText[4] = "Maintenant, c'est toi qui doit aller te mettre en sécurité.."
myText[5] = "Non, je voudrais me battre à vos côté pour le village! "
myText[6] = "Tu ne peux pas. C'est trop dangereux ici. Il y a..."

myText2[0] = "C'est bon elle est là"
myText2[1] = "Purakushisu, reste en arrière c'est trop dangereux."

myText3[0] = "Tiens, tiens. Mais si ce n'est pas le fameux penguins qui a réussi à fuir son univers avant sa destruction. "
myText3[1] = "Dorufunonga, que fais-tu ici? "
myText3[2] = "Rien, je ne fait que détruire les villages qui ne veulent pas se soumettrent à ma dictature. Il n'y a rien de mal à ça. C'est... le pouvoir. "
myText3[3] = "Mais d'ailleurs, ça m'étonne que tu soit dans un village perdu au millieu de nul part. Je pensais que tu allais être dans des endroits moins... ruraux "
myText3[4] = "J'aime bien tout ce qui est traditionnel. "
myText3[5] = "Mais cesse de plaisanter, il est temps pour toi de fuir cette terre. "

myText4[0] = "C'est bien joué mais tes attaques basiques ne servent à rien. Tu te bat éxactement comme un maître ninja penguin qui joue aux cartes."
myText4[1] = "Je vais te montrer à quoi ressemble une vrai attaque."

myText5[0] = "Je peux avouer que tu m'a bien eu sur ce coup là. Tu m'a sacrément amoché";
myText5[1] = "Ce n'est rien par rapport à ce que je peux faire. Je vais t'en faire une demonstration.";

myText6[0] = "NANI!?!?!?!?!??!?!?!?!?!?!?!?!??!?!?!?!?!??!?!??!?!?!?!?!";

myText7[0] = "Déjà à terre? Je ne savais pas que j'allais te battre aussi vite. "
myText7[1] = "Je vois que je t'ai infligé une blessure mortelle. Tu n'as aucune chance de t'en sortir."
myText7[2] = "Mais si tu veux, je peux abréger tes souffrances. "

myText8[0] = "NOOOOOOON!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!";

myText9[0] = "Je ne te laisserai pas le toucher une fois de plus avec tes attaques.";
myText9[1] = "Hmmm, une jeune humaine s'oppose à moi. Je ne gaspillerai pas ma puissance pour elle.";
myText9[2] = "Vous deux. Venez ici.";

myText10[0] = "Hey Samurai No.2, viens ici, le maître nous a appelé.";
myText10[1] = "Ah ok j'arrive";

myText11[0] = "C'est bon je suis là.";
myText11[1] = "Parfait! Maintenant tuez cette vermine et ensuite, rejoignez moi au repère.";
myText11[2] = "C'est comme si c'était fait.";

myText12[0] = "Éloignez-vous!!!";
myText12[1] = "Hehe, je ne crois pas que tu a compris jeune fille. Tu n'es pas en position de nous faire peur.";
myText12[2] = "J'ai dis ÉLOIGNEZ VOUS!!!!!!";
myText12[3] = "Oulah j'ai l'impression qu'elle maîtrise les mêmes pouvoir que notre maître. On devrait peut-être l'écouter finalement.";
myText12[4] = "Est-ce que tu compte vraiment fuir? Tu te rend compte que si tu fuis notre maître va te tuer. Dans tout les cas on est fichus.";

myText13[0] = "Hé!!! Reviens!!!";
myText13[1] = "Bon, on dirai que c'est un 1 v 1. Ce n'est pas très équilibré pour toi. Si tu veux, tu peux toujours abandonner.";
myText13[2] = "Katana no kakera de shinu!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!";
myText13[3] = "... et c'est à ce moment que vous devez insérer le meme: ''it was that moment jackson knew, he fucked up''";

myText14[0] = "Bon peut-etre que Samurai No.2 avait raison. Je devrait peut-être fuir moi aussi.";

myText15[0] = "Qu'est-ce qu'il m'arrive?";
myText15[1] = "Sensei!";

myText16[0] = "Sensei! Est-ce que vous m'entendez?!";
myText16[1] = "Purakushisu...";
myText16[2] = "Sensei! Je vais vous portez jusqu'aux autres villageaois. On va vous soigner.!";
myText16[3] = "Non, il est trop tard. Je ne tiendrais pas assez longtemps. Mais il est temps que tu sache toute la vérité avant que tu meurt.";
myText16[4] = "Purakushisu... Cette dame qui m'a infligé cette blessure s'appelle dorufunonga. C'est un démon. ";
myText16[5] = "Vous voulez dire, comme celui qui a détruit votre univers Sensei?";
myText16[6] = "Effectivement. Ils possèdent des pouvoirs aux potentiels immenses. ";
myText16[7] = "Mais leurs soifs de pouvoirs les poussent à semer le chaos et la destruction sur leurs passages. "
myText16[8] = "Un mortel normal comme moi ne fait pas long feux face à une puissance comme celles-ci.";

myText17[0] = "Cependant, il existe des êtres qui sont, selon moi, capables de se battre contre eux. ";
myText17[1] = "Je les appelle... les dark warrior. ";
myText17[2] = "Ce sont des guerriers qui sont dotés de pouvoirs extraordinaires et qui sont capables de manipuler une sorte d'energie noire. ";
myText17[3] = "Chaques univers possèdent son propre dark warrior. ";
myText17[4] = "Il y a fort longtemps, avant même que tu naisse, à l'epoque où que je vivais dans mon univers natal, je cotoyai un dark warrior. ";
myText17[5] = "Son nom était Antoine Bouc. Il était très fort au card jitsu comme toi.";
myText17[6] = "Malheureusement, je ne l'ai jamais revu depuis la destruction de mon univers.";
myText17[7] = "Les dark warrior maîtrisent une energie noire.";
myText17[8] = "Un peu comme mon attaque que j'ai déclenché accidentellement sur ce samurai.";
myText17[9] = "Éxactement! et c'est pour ça que je crois que tu es la dark warrior de cet univers.";
myText17[10] = "Purakushisu... Tu es une decendante de Iyou, la déesse des ténèbres.";
myText17[11] = "Tu possède une partie de ses pouvoirs sans même à avoir besoin d'une Shadow Blade.";
myText17[12] = "Je suis une dark warrior parce que je suis la decendante de Iyou.";
myText17[13] = "Cela signifie que dans tous les univers, Iyou a des decendances qui sont des dark warrior?.";
myText17[14] = "Pas vraiment. Généralement, les dark warriors sont des personnes banales qui sont choisis par leurs propres destins.";
myText17[15] = "Mais ils ne possèdent pas de pouvoir de base ou d'autres facultées du genre.";
myText17[16] = "Ils ont besoins d'une shadow blade pour utiliser leurs pouvoirs.";
myText17[17] = "Toi tu possède déjà ces pouvoirs, mais tu ne peux pas les contrôler si tu ne possède pas de shadow blade.";
myText17[18] = "Mais qu'es-ce qu'une shadow blade?";


myText18[0] = "Les shadows blades sont des lames forgées avec le métal les plus solides du dark world.";
myText18[1] = "Ce métal est capable de conserver en mémoire les pouvoirs que Iyou met dans la lame";
myText18[2] = "Les lights blades sont vraiment semblables, mais ils sont fait avec un métal du light world et conservent les pouvoirs de phok.";
myText18[3] = "Lorsque tu possède les deux lames, tu peux voyager entre les univers comme tu le souhaite."
myText18[4] = "Et je crois que Dorufunonga possède les deux lames"
myText18[5] = "Mais je connais l'emplacement d'une lame. Le Dakukatana"
myText18[6] = "Où ca?"
myText18[7] = "Le Dakukatana se situe dans un temple perdu au fin fond des terres de l'Ether, un autre monde. (Dans la de Praxis, il y a une beaucoup de monde)"
myText18[8] = "C'est tout ce que je sais"

myText19[0] = "En ce moment, je sens mes forces s'épuiser."
myText19[1] = "J'ai demandé au reste du village à aller se réfugier au village de Monuterunelu."
myText19[2] = "Vas-y et va demander au moine Shinso des informations sur Dorufunonga. Il va t'en fournir."
myText19[3] = "Mais, je ne peux pas vous abandonner comme ça. Il y a toujours une chance de vous sauver."
myText19[4] = "Non, c'est trop tard. Je crois bien que c'est la fin pour moi."
myText19[5] = "J'ai vraiment apprécié vous apprendre ce que je sais, à toi et tes amis."
myText19[6] = "Et j'espère que tu auras une formidable vie..."
myText19[7] = "Adieu."

myText20[0] = "SENSEEEEEEI!!!!!!!!!!!!!!!!!!"

myText21[0] = "Je devrais aller au village de Monuteruneru au nord d'ici. "
myText21[0] = "Elle va payer pour ce qu'elle a fait. "

